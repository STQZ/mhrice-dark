{
  "devDependencies": {
    "bulma": "^0.9.4",
    "eslint": "^8.17.0",
    "node-sass": "^8.0.0"
  },
  "scripts": {
    "css-build": "node-sass --output-style compressed --include-path node_modules --omit-source-map-url src/extract/static/style/mhrice.scss src/extract/static/mhrice.css",
    "css-watch": "npm run css-build -- --watch",
    "start": "npm run css-watch"
  }
}
